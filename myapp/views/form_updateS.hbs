{{{body}}}
  <div class="container mt-5">
    <form class="row g-3" action="#" method="post" enctype="application/x-www-form-urlencoded">
      <div class="col-md-4">
        <label for="inputImagem" class="form-label">Imagem</label>
        <input type="file" class="form-control" id="imagem" name="imagem" accept="image/*">
      </div>
      <div class="col-md-4">
        <label for="nomeCompleto" class="form-label">Nome Completo</label>
        <input type="text" value="{{dados.nomeCompleto}}" class="form-control" id="nomeCompleto" name="nomeCompleto">
      </div>
      <div class="col-md-4">
        <label for="idade" class="form-label">Idade</label>
        <input type="number" value="{{dados.idade}}" class="form-control" id="idade" name="idade">
      </div>
      <div class="col-md-4">
        <label for="dataNascimento" class="form-label">Data de Nascimento</label>
        <input type="date" value="{{dados.dataNascimento}}" class="form-control" id="dataNascimento" name="dataNascimento">
      </div>
      <div class="col-md-4">
        <label for="sexo" class="form-label">Sexo</label>
        <select id="sexo" {{dados.sexo}} class="form-select" name="sexo">
          <option  >Masculino</option>
          <option  >Feminino</option>
          <option  selected>Outro</option>
        </select>
      </div>
      <div class="col-md-4">
        <label for="tatoo" class="form-label">Tatuagem</label>
        <select id="tatoo" {{dados.tatoo}} class="form-select" name="tatoo">
          <option selected>Nenhuma</option>
          <option >Perna Esquerda</option>
          <option >Perna Direita</option>
          <option >Braço Esquerdo</option>
          <option >Braço Direito</option>
          <option >Torax</option>
          <option >Pescoço</option>
          <option >Costas</option>
        </select>
      </div>
      <div class="col-md-4">
        <label for="genitora" class="form-label">Nome da Genitora</label>
        <input type="text" value="{{dados.genitora}}" class="form-control" id="genitora" name="genitora">
      </div>
      <div class="col-md-4">
        <label for="genitor" class="form-label">Nome do Genitor</label>
        <input type="text" value="{{dados.genitor}}" class="form-control" id="genitor" name="genitor">
      </div>
      <div class="col-md-4">
        <label for="faccoes" class="form-label">Faccoes</label>
        <input type="text" value="{{dados.faccoes}}" class="form-control" id="faccoes" name="faccoes">
      </div>
      <div class="col-md-4">
        <label for="numero" class="form-label">Número</label>
        <input type="text" value="{{dados.numero}}" class="form-control" id="numero" name="numero">
      </div>
      <div class="col-md-4">
        <label for="rua" class="form-label">Rua</label>
        <input type="text" value="{{dados.rua}}" class="form-control" id="rua" name="rua">
      </div>
      <div class="col-md-4">
        <label for="bairro" class="form-label">Bairro</label>
        <input type="text" value="{{dados.bairro}}" class="form-control" id="bairro" name="bairro">
      </div>
      <div class="col-md-6">
        <label for="local" class="form-label">Local do s13/abordagem</label>
        <input type="text" value="{{dados.local}}" class="form-control" id="local" name="local">
      </div>
      <div class="col-12">
        <button type="submit" class="btn btn-primary">Enviar</button>
      </div>
    </form>
  </div>

